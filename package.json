	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/node": "^20.11.5",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=8.0.0"
	}
}
