		"@typescript-eslint/parser": "^6.21.0",
		"@vitest/ui": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^8.57.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.5.6",
